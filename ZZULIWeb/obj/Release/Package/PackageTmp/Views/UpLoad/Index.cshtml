            <!--webkitdirectory-->
            课程文件：<input style="margin-bottom:10px;" type="file"  id="Moves" /><br>
            课程名字：<input type="text" id="Cou_Name" value="" /><br>
            @*课程标题：<input type="text" value="" id="Cou_Title" /><br>*@
            方向:
            <select id="lb_nav0" name="Nav0">
                <option value="-1">--未选择--</option>
                @{
                    List<Navigations> list = new Navigation().GetNavigation0();
                    foreach (Navigations nav in list)
                    {
                        <option value="@nav.Nav_ID">@nav.Nav_Name</option>
                    }
                }
                @*<option>后台</option>
                <option>JAVA</option>
                <option>C</option>
                <option>.net</option>
                <option>Ｃ++</option>*@
            </select>
            分类:
            <select id="lb_nav1" name="Nav1">
                <option value="-1">--未选择--</option>
            </select>
            @*@foreach (Navigations nav in ViewBag.TagsList)
            {

                <input class="tags" type="button" value="@nav.Nav_Name" />
            }*@
            @*类型:
            <select>
                <option>基础</option>
                <option>框架</option>
                <option>工具</option>
            </select>*@<br>
            简介：<br>
            <textarea rows="6" cols="80" id="Cou_Describe"></textarea><br>
            图片：<input type="file" accept=".png" value="浏览..." id="Cou_Img" /><br>
            <input style="padding:4px 6px; cursor:pointer; outline:none; border:none; font-size:16px; color:#808080; margin-top:10px;" id="uploadbtn" type="button" value="提交" />
        </form>

    </div>
</div>


<div id="wrapper">
